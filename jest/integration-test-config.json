{
  "testEnvironment": "node",
  "roots": [
    "src"
  ],
  "transform": {
    ".*\\.js$": "<rootDir>/jest/preprocessor.js"
  },
  "setupTestFrameworkScriptFile": "<rootDir>/jest/integration-test-setup.js",
  "moduleFileExtensions": [
    "js",
    "json",
    "node"
  ],
  "testRegex": "__integration_tests__/.*\\.js$"
}
